Order params
<br/>
<table style='border: 1px solid black;'>
  <th>Param</th>
  <th>Value</th>
  <% @params.each do |key, val| %>
  <tr>
    <td><%= key %></td>
    <td><%= val %></td>
  </tr>
  <% end %>
</table>
<br/>
<%= link_to 'New order', "#{APP_CONFIG[:id_net_url]}/checkout?#{@params.to_param}", target: '_blank' %>